
(rules PCB TopBreakout
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 131)
    (layer_rule Top
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule Bottom
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 125.0)
    (clear 125.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 180.2 (type default_PWR))
    (clear 31.4 (type smd_smd))
    (clear 180.2 (type smd_PWR))
    (clear 180.2 (type "kicad_default"_PWR))
    (clear 180.2 (type PWR_PWR))
  )
  (padstack "Via[0-1]_400:250_um"
    (shape
      (circle Top 400.0 0.0 0.0)
    )
    (shape
      (circle Bottom 400.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_500:300_um"
    (shape
      (circle Top 500.0 0.0 0.0)
    )
    (shape
      (circle Bottom 500.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_400:250_um" "Via[0-1]_400:250_um" default
  )
  (via 
    "Via[0-1]_500:300_um" "Via[0-1]_500:300_um" default
  )
  (via 
    "Via[0-1]_400:250_um-kicad_default" "Via[0-1]_400:250_um" "kicad_default"
  )
  (via 
    "Via[0-1]_500:300_um-kicad_default" "Via[0-1]_500:300_um" "kicad_default"
  )
  (via 
    "Via[0-1]_400:250_um-PWR" "Via[0-1]_400:250_um" PWR
  )
  (via 
    "Via[0-1]_500:300_um-PWR" "Via[0-1]_500:300_um" PWR
  )
  (via_rule
    default "Via[0-1]_400:250_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_400:250_um-kicad_default"
  )
  (via_rule
    PWR "Via[0-1]_500:300_um-PWR"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 125.0)
    )
    (circuit 
      (use_layer Top Bottom)
    )
  )
  (class "kicad_default"
    "DA_V1" "DA_V2" "DA_V3" "DA_V4" "TFT_CS" "TFT_DC" "SPI0_SCK" "SPI0_MOSI"
    "AD_BUSY" "AD_RESET" "AD_CONVST" "AD_CS" "SPI0_MISO" "DA_SYNC" "/AD7606/AD_REGCAP2" "/AD5754 DAC/VREF"
    "/AD7606/AD_REGCAP1" "/AD7606/AD_REFCAP_A" "/FPC connector/USB_B_D+" "/FPC connector/USB_B_D-" "/FPC connector/USB_A_D-" "/FPC connector/USB_A_D+" "/AD7606/AD_BYTE_SELECT" "/AD7606/AD_REFSEL"
    "/AD7606/AD_RANGE" "/AD7606/AD_STBY" "/AD7606/AD_OS0" "/FPC connector/SD_DAT1" "/FPC connector/SD_DATAOUT" "/FPC connector/SD_CLK" "/FPC connector/SD_DATAIN" "/FPC connector/SD_CS"
    "/FPC connector/SD_DAT2" "GATE_1" "GATE_2" "GATE_3" "GATE_4" "MIDI_OUT_PIN5" "MIDI_IN_PIN5" "MIDI_OUT_PIN4"
    "MIDI_TX" "MIDI_RX" "MIDI_IN_PIN4" "Net-(D1-Pad1)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 125.0)
    )
    (circuit 
      (use_layer Top Bottom)
    )
  )
  (class PWR
    GND "AD_V2" "AD_V3" "AD_V4" "AD_V1" VIN AVDD AVSS
    /AD7606/AVIN "/FPC connector/USB_B_VCC" "/FPC connector/USB_A_VCC" "/AD7606/AD_AGND" "/FPC connector/SD_GND" "/FPC connector/SD_VCC" "AD_V5" "AD_V6"
    "AD_V7" "AD_V8" +3V3
    (clearance_class PWR)
    (via_rule PWR)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer Top Bottom)
    )
  )
)